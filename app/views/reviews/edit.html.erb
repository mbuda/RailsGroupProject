<%= content_for :title, "Edit a review" %>
<div class="container">
  <h1>Editing review</h1>
  <div class="well">
    <div class="btn btn-info">
      <%= link_to 'Go back', game_reviews_path %>
    </div>
    <div class="btn btn-danger">
      <%= link_to 'Delete it', [@game, @review], method: :delete, data: {confirm: "Are you sure?"} %>
    </div>
    <%= render 'form' %>
  </div>
</div>
